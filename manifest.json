{

  "manifest_version": 2,
  "name": "Callify",
  "version": "1.0",

  "description": "click to dail",
  "icons": {
    "48": "icons/callify.svg"
  },

  "permissions": [
    "contextMenus",
    "webRequest"
  ],

  "browser_action": {
    "default_icon": "icons/callify.svg",
    "default_title": "Callify"
  },
  "background": {
    "scripts": ["content_scripts/callify.js"]
  }
}
